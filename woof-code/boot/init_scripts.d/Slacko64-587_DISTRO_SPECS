#One or more words that identify this distribution:
DISTRO_NAME='Slacko64 Puppy'
#version number of this distribution:
DISTRO_VERSION=5.8.7
#The distro whose binary packages were used to build this distribution:
DISTRO_BINARY_COMPAT='slackware64'
#Prefix for some filenames: exs: slacko64save.2fs, slacko64-5.8.7.sfs
DISTRO_FILE_PREFIX='slacko64'
#The version of the distro whose binary packages were used to build this distro:
DISTRO_COMPAT_VERSION='14.0'
#the kernel pet package used:
DISTRO_KERNEL_PET='linux_kernel-3.12.7-slacko64.pet'
DISTRO_TARGETARCH='x86_64'
DISTRO_XORG_AUTO='yes'
DISTRO_DB_SUBNAME='slacko6414'
#32-byte alpha-numeric ID-string appended to vmlinuz, puppy_slacko64_5.8.7.sfs, zdrv_slacko64_5.8.7.sfs and devx.sfs:
DISTRO_IDSTRING='s140115153903ZZZZ5.8.7XXXXXXXXXX'
#Puppy default filenames...
#Note, the 'SFS' files below are what the 'init' script in initrd.gz searches for,
#for the partition, path and actual files loaded, see PUPSFS and ZDRV in /etc/rc.d/PUPSTATE
DISTRO_PUPPYSFS='puppy_slacko64_5.8.7.sfs'
DISTRO_ZDRVSFS='zdrv_slacko64_5.8.7.sfs'
DISTRO_PUPPYDATE='Jan 2014'

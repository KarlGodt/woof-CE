/dev/sda6		/				defaults		0	1
/dev/sdc6     /            ext4     defaults        0   1
/dev/sdb6     /            ext4     defaults        0   1
proc          /proc        proc     defaults               0 0
sysfs         /sys         sysfs    defaults               0 0
devpts        /dev/pts     devpts   gid=2,mode=620         0 0


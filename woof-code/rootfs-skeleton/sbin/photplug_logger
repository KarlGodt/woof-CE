#!/bin/ash
. /etc/rc.d/f4puppy5
# BATCHMARKER01 - Marker for Line-Position to bulk insert code into.

eval `env`
[ "$ACTION" = remove ] && SUFFIX=rem || SUFFIX=add
echo "
$0 : $$ $PPID
`ps-FULL -o pid,ppid,s,args | grep -w -E \"${$}|${PPID}\"`
---------- $@ --------------
`date +%T:%N`

`env`

`dmesg|tail`
">> "/tmp/hotplug.${SUFFIX}.log"

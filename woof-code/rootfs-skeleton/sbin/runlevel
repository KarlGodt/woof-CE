#!/bin/sh
# Puppy/Woof does not use runlevels. /etc/init.d/dbus (in Ubuntu) runs 'runlevel'
# Command at startup. um, so i am providing a dummy here (it is normally
# in the sysvinit pkg)...

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

TWO_HELP=''; TWO_VERSION=''; TWO_VERBOSE=''; TWO_DEBUG=''; ## Set to anything if code requires further down (ie. custom usage or version message)
ADD_HELP_MSG=""
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
    for i in `seq 1 1 $DO_SHIFT`; do shift; done; }
_trap
}

#detect if a chrooted environment...
if [ ! -e /proc/mounts ];then
 echo "unknown"
 exit
fi

XSTATUS="no"
[ -z $DISPLAY ] || XSTATUS="yes"

if [ "$XSTATUS" = "no" ];then
 echo "2 3" #previous and current runlevels.
else
 echo "3 5"
fi

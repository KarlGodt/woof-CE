_do_fbsplash(){
[ "$FBSPLASH_DONE" ] && return 1
PICURE=/boot/grub/boot-splash.ppm
test "$1" && PICTURE="$1"
test -f "$PICTURE" || return 2
DEVICE=/dev/fb0
test "$2" && DEVICE="$2"
test -e "$DEVICE" || return 3
fbsplash -s "$PICTURE" -d "$DEVICE"
if test $? = 0; then
FBSPLASH_DONE=1
return 0
fi
return $?
}
_do_fbsplash "" "";echo $?
_do_fbsplash "";echo $?
_do_fbsplash;echo $?
echo $FBSPLASH_DONE
_do_fbsplash;echo $?
exit

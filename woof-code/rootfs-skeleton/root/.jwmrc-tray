<JWM>
    <Tray  autohide="false" insert="right" x="0" y="0" border="1" height="28" layout="horizontal" halign="right" valign="top" width="1280" >
        <!-- Additional TrayButton attribute: label -->
        <TrayButton label="Menu" icon="mini-dog.xpm">root:3</TrayButton>
         <!-- <TrayButton popup="Run commandline" icon="gexec.xpm">exec:gexec</TrayButton> -->
         <TrayButton popup="RXVT" icon="/usr/local/lib/X11/mini-icons/mini-term.xpm">exec:rxvt</TrayButton>
         <TrayButton popup="abiword" icon="/usr/local/lib/X11/mini-icons/abiword.xpm">exec:abiword</TrayButton>
         <TrayButton popup="geany" icon="/usr/local/lib/X11/mini-icons/mini-edit.xpm">exec:geany</TrayButton>
         <TrayButton popup="Show Desktop" icon="mini-desktop.xpm">showdesktop</TrayButton>
         <TrayButton popup="snap" icon="/usr/local/lib/X11/mini-icons/mini-camera.xpm">exec:mtpaintsnapshot.sh</TrayButton>
        <TrayButton popup="ROOTFS" icon="/usr/local/lib/X11/pixmaps/folder24.png">exec:/usr/local/bin/rox</TrayButton>
         <!-- Additional Pager attributes; width, height -->
        <Pager/>
        <!-- Additional TaskList attribute: maxwidth -->
        <TaskList/>
        <Dock/>
        <!-- Additional Swallow attribute: height -->
        <Swallow name="blinky">
            blinkydelayed -bg "#ffc100"
        </Swallow>
        <Swallow name="asapm">
            asapmshell -u 4
        </Swallow>
        <Swallow name="freememapplet" width="34">
            freememappletshell
        </Swallow>
        <Swallow name="xload" width="32">
xload -nolabel -fg red -hl white -bg "#E8E8E8"
        </Swallow>
        <Clock>minixcal</Clock>
    </Tray>
</JWM>

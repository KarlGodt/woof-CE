<JWM>
    <Tray  autohide="false" insert="right" x="0" y="-1" border="1" layout="horizontal" halign="left" valign="bottom" width="1280" height="28" >
        <!-- Additional TrayButton attribute: label -->
        <TrayButton label="Menu" icon="mini-dog.xpm">root:3</TrayButton>
         <!-- <TrayButton popup="Run commandline" icon="gexec.xpm">exec:gexec</TrayButton> -->
        <TrayButton popup="Show Desktop" icon="mini-desktop.xpm">showdesktop</TrayButton>
        <TrayButton popup="Take Screenshot" icon="/usr/local/lib/X11/mini-icons/gtkam.png">exec:mtpaintsnapshot.sh</TrayButton>
        <TrayButton popup="Sakura" icon="/usr/local/lib/X11/mini-icons/hicolor/16x16/apps/gimp.png">exec:sakura</TrayButton>
        <TrayButton popup="rox" icon="/usr/local/lib/X11/themes/Tok-tok/folder24.png">exec:/usr/local/bin/rox</TrayButton>
        <TrayButton popup="geany" icon="/usr/local/lib/X11/mini-icons/gtk-edit.png">exec:/usr/bin/geany</TrayButton>
        <!-- Additional Pager attributes; width, height -->
        <Pager/>
        <!-- Additional TaskList attribute: maxwidth -->
        <TaskList/>
        <Dock/>
        <!-- Additional Swallow attribute: height -->
        <!-- 722
        <Swallow name="glipper">
            glipper
        </Swallow>
        <Swallow name="fbxkb">
            fbxkb
        </Swallow>
        <Swallow name="absvolume">
            absvolume
        </Swallow>
        <Swallow name="retrovol">
            retrovol -hide
        </Swallow>
        -->
        <Swallow name="blinky">
           blinkydelayed -bg "#DCDAF5"
        </Swallow>
        <Swallow name="asapm">
            asapmshell -u 4
        </Swallow>
        <Swallow name="freememapplet" width="34">
            freememapplet
        </Swallow>
        <Swallow name="xload" width="32">
xload -nolabel -fg red -hl white -bg "#888888"
        </Swallow>
        <Clock format="%I#%M">minixcal</Clock>
    </Tray>
</JWM>

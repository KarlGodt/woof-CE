<JWM>
        <!-- <Tray layer="2" layout="vertical" autohide="false" insert="right" x="0" y="30" border="1" height="9999" width="30" halign="top" valign="top" >  /* tray underneath windows */ -->

        <!-- <Tray layer="0" layout="vertical" autohide="false" insert="right" x="0" y="30" border="1" height="9999" width="30" halign="right" valign="top" > /*tray underneath windows , tray right side on screen */ -->

        <!-- <Tray layer="1" layout="vertical" autohide="false" insert="right" x="0" y="30" border="1" height="9999" width="30" halign="right" valign="top" > -->
        <!-- orig <Tray  autohide="false" insert="right" x="0" y="1" border="1" height="1024" >  -->
        <Tray autohide="false" insert="left" x="0" y="1" border="1" height="1024" valign="top" halign="right" layout="vertical" width="48" >

                <!-- Additional TrayButton attribute: label -->
                <TrayButton label="Menu" icon="mini-dog.xpm">root:3</TrayButton>
<!-- <TrayButton popup="Run commandline" icon="gexec.xpm">exec:gexec</TrayButton> -->

                <TrayButton popup="SCREEENIIEEE" icon="/usr/local/lib/X11/mini-icons/mini-camera.xpm">exec:mtpaintsnapshot.sh</TrayButton>
                <TrayButton popup="Choose Default Browser" icon="www24.png">exec:browser-default</TrayButton>
                <TrayButton popup="Free space in drives" icon="mini-hdisk.xpm">exec:partview</TrayButton>
                <TrayButton popup="SAKURA" icon="/usr/local/lib/X11/themes/PuppyGANT/console48.png">exec:sakura</TrayButton>
                <TrayButton popup="Show Desktop" icon="mini-desktop.xpm">showdesktop</TrayButton>
                <TrayButton popup="ROX" icon="/usr/local/lib/X11/themes/Tok-tok/folder48.png">exec:rox</TrayButton>

                <!-- Additional Pager attributes; width, height -->
                <Pager width="-1" hight="0" />
                <!-- Additional TaskList attribute: maxwidth -->
                <TaskList maxwidth="100" />
                <Dock />
                <!-- Additional Swallow attribute: height -->
                <Swallow name="fbxkb">
                        fbxkbshell
                </Swallow>
                <Swallow name="blinky">
                        blinkydelayed -bg "#d3d7cf"
                </Swallow>
                <Swallow name="asapm">
                        asapmshell -u 4
                </Swallow>
                <Swallow name="freememapplet" width="34">
                        freememappletshell
                </Swallow>
                <Swallow name="xload" width="32">
                        xload -nolabel -fg red -hl white -bg "#d3d7cf"
                </Swallow>
                <Clock format="%a%b%d %R">minixcal</Clock>
        </Tray>
</JWM>

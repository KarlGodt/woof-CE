[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in `seq 1 1 $DO_SHIFT`; do shift; done; }

_trap
}

for x in cancel lp lpq lpr lprm lpstat; do
    if [ ! -e /usr/bin/$x ];then
    echo "/usr/bin/$x does not exist"
if [ -f /usr/bin/${x}_cups ];then
    echo "/usr/bin/${x}_cups does exist"
    echo "$0: Re-creating /usr/bin/$x -> ${x}_cups ..."
    echo -e "#!/bin/sh\nexec -a $x ${x}_cups \"\$@\"" > /usr/bin/$x
    chmod +x /usr/bin/$x
fi
    else
if [ ! -f /usr/bin/${x}_cups ];then
     echo "/usr/bin/${x}_cups does NOT exist"
     mv /usr/bin/$x /usr/bin/${x}_cups
    echo -e "#!/bin/sh\nexec -a $x ${x}_cups \"\$@\"" > /usr/bin/$x
    chmod +x /usr/bin/$x
fi
    fi
done
for x in lpc; do
    if [ [ ! -e /usr/sbin/$x ];then
    echo "/usr/sbin/$x does not exist"
    if  -f /usr/bin/${x}_cups ];then
    echo "$0: Re-creating /usr/sbin/$x -> ${x}_cups ..."
    echo -e "#!/bin/sh\nexec -a $x ${x}_cups \"\$@\"" > /usr/sbin/$x
    chmod +x /usr/sbin/$x
    fi
    else
     if [ ! -f /usr/sbin/${x}_cups ];then
     echo "/usr/sbin/${x}_cups does NOT exist"
     mv /usr/sbin/$x /usr/sbin/${x}_cups
    echo -e "#!/bin/sh\nexec -a $x ${x}_cups \"\$@\"" > /usr/sbin/$x
    chmod +x /usr/sbin/$x
     fi
     i
done
exit 0


#!/bin/ash
. /etc/rc.d/f4puppy5
# BATCHMARKER01 - Marker for Line-Position to bulk insert code into.

SLEEP=1 #seconds
#try prevent freememapplet escaping tray when jwm restarted:
if [ "`pidof freememapplet inotifywait`" ]; then
killall freememapplet inotifywait 2>>$ERR
sleep $SLEEP
fi

exec freememapplet "$@" &

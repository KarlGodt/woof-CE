#!/bin/bash
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_func_kill"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/pmusic/func_kill"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in 1; do shift; done; }

_trap

}
# End new header
#
IFS=$'\n'

TMP="`ps`"
for I in `echo "$TMP" | grep "aplay" | awk '{print $1}'`; do kill -9 $I 2> /dev/null; done
for I in `echo "$TMP" | grep "ffmpeg" | awk '{print $1}'`; do kill -9 $I 2> /dev/null; done
for I in `echo "$TMP" | grep 'func_player -playing' | awk '{print $1}'`; do kill -9 $I 2> /dev/null; done
killall cdda2wav# Very End of this file 'usr/local/pmusic/func_kill' #
###END###

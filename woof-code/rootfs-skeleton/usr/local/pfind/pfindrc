#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_pfindrc"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/pfind/pfindrc"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
[ "$HAVE_F4PUPPY5" ] || source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

TWO_HELP=''; TWO_VERSION=''; TWO_VERBOSE=''; TWO_DEBUG=''; ## Set to anything if code requires further down (ie. custom usage or version message)
ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in `seq 1 1 $DO_SHIFT`; do shift; done; }

_trap

}
# End new header
#
export LANGUAGE=auto
export FILENAME=Hybrid
ACCEPT_ARROW=false
export MAX_LIST_ITEMS=500
export FILEMANAGER="rox"
export TEXTEDITOR="defaulttexteditor"
export EXEC_FILEMANAGER="rox /tmp/searchresult/"
export EXEC_TEXTEDITOR="defaulttexteditor /tmp/searchresult/*"
export EXEC_EXPORT_LIST_TO_TEXTEDITOR="defaulttexteditor /tmp/pfind-text"
export EXEC_IMAGEBROWSER="viewnior /tmp/searchresult"
export EXEC_SLIDESHOW="viewnior /tmp/searchresult"
export EXEC_MUSICPLAYER="aqualung /tmp/pfind-music.m3u"
export EXEC_VIDEOPLAYER="gxine /tmp/searchresult/*"
export EXEC_BURNER="pburn -i /tmp/pfind-view"
export RADIOBUTTON_PATH_PUPPY=false
export RADIOBUTTON_PATH_MNT=true
export RADIOBUTTON_PATH_CURRENT=false
export RADIOBUTTON_PATH_ALL=false
export PATH_PUPPY='/bin [OR] /etc [OR] /lib [OR] /opt [OR] /sbin [OR] /tmp [OR] /usr [OR] /var'
export PATH_MNT='/mnt/ [OR] /root'

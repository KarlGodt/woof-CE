#!/bin/bash
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_mplayerplaylist"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/EmbeddedBookmarks/mplayerplaylist"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in 1; do shift; done; }

_trap

}
# End new header
#

echo "$1" > /tmp/mplayerplaylist

if [ -f /usr/local/bin/mplayer-git ];then

	mplayer-git -quiet -playlist /tmp/mplayerplaylist
	exit 0
fi


mplayer -quiet -playlist /tmp/mplayerplaylist

# Very End of this file 'usr/local/EmbeddedBookmarks/mplayerplaylist' #
###END###

#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_gtkmoz"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/bin/gtkmoz"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
[ "$HAVE_F4PUPPY5" ] || source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

TWO_HELP=''; TWO_VERSION=''; TWO_VERBOSE=''; TWO_DEBUG=''; ## Set to anything if code requires further down (ie. custom usage or version message)
ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in `seq 1 1 $DO_SHIFT`; do shift; done; }

_trap

}
# End new header
#
#BK June 2009

export MOZ_DISABLE_PANGO=1

#grab these from SeaMonkey...
[ ! -f /root/.PuppyBrowser/default/cert8.db ] && /usr/local/PuppyBrowser/find_certs &

cd /usr/local/PuppyBrowser

if [ ! $1 ];then
 exec ./PuppyBrowser "file:///usr/share/doc/home.htm" -title="PuppyBrowser" -w=796 -h=580
fi

if [ $2 ];then
 exec ./PuppyBrowser $@
else
 exec ./PuppyBrowser "${1}" -title="PuppyBrowser" -w=796 -h=580
fi
# Very End of this file 'usr/local/bin/gtkmoz' #
###END###

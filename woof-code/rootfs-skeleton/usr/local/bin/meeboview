#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_meeboview"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/bin/meeboview"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in 1; do shift; done; }

_trap

}
# End new header
#

if [ -e /usr/local/bin/bareview ];then
 export MOZ_DISABLE_PANGO=1
 /usr/local/bin/bareview http://wwwm.meebo.com/ &
 #  -placement top
 yaf-splash -timeout 6 -font "8x16" -outline 0 -margin 4 -bg orange -text "Connecting to meebo.com, please wait..."
 exit
fi

exec defaultbrowser http://wwwm.meebo.com/

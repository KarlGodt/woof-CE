#!/bin/ash

DEFAULT_AUDIOMIXER=zmixer

if [ "`which retrovol`" ]; then
 if [ "`pidof retrovol`" ]; then :; else exec retrovol "$@"; fi
fi

if [ "`whilch alsamixer`" ]; then
 if [ "`pidof alsamixer`" ]; then :; else exec alsamixer "$@"; fi
fi

if [ "`whilch zmixer`" ]; then
 if [ "`pidof zmixer`" ]; then :; else exec zmixer "$@"; fi
fi

#exec zmixer "$@"
exec $DEFAULT_AUDIOMIXER "$@"

#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_path"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/jwmconfig3/path"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
[ "$HAVE_F4PUPPY5" ] || source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

TWO_HELP=''; TWO_VERSION=''; TWO_VERBOSE=''; TWO_DEBUG=''; ## Set to anything if code requires further down (ie. custom usage or version message)
ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in `seq 1 1 $DO_SHIFT`; do shift; done; }

_trap

}
# End new header
#
PRO="defaults"

#jwmConfigMgr
MINI_ICONS_DIR="/usr/local/lib/X11/mini-icons"
HOME_DIR="$HOME/.jwm"
TMP_DIR=/tmp/jwmconfig
test -d "$TMP_DIR" || mkdir -p "$TMP_DIR"

trayFILE="$HOME/.jwmrc-tray"
trayFILEbak="$HOME/.jwmrc-tray-bak"
trayFILEtmp="$HOME/.jwmrc-tray-temp"
trayFILEdef="$HOME/.jwmrc-tray-def"
tmpTRAY="$TMP_DIR/jwmrc-tray"

personalFILE="$HOME/.jwm/jwmrc-personal"
personalFILE2="$HOME/.jwm/jwmrc-personal2"
personalFILEbak="$HOME/.jwm/jwmrc-personal-bak"
personalFILEtmp="$HOME/.jwm/jwmrc-personal-tmp"

colorFILE="$HOME/.jwm/jwm_colors"
colorFILEbak="$HOME/.jwm/jwm_colors-bak"
colorFILEtmp="$HOME/.jwm/jwm_colors-tmp"
colorFILEdef="$HOME/.jwm/jwm_colors-def"

themeFILE="$HOME/.jwm/jwmrc-theme"
themeFILEbak="$HOME/.jwm/jwmrc-theme-bak"
themeFILEtmp="$HOME/.jwm/jwmrc-theme-tmp"
themeFILEdef="$HOME/.jwm/jwmrc-theme-def"
themeFILEdir="$HOME/.jwm/themes"
themeDIR="$HOME/.jwm/themes"

DBG="debug $SCRIPT_DIR/$PRO pid=$$ line>"

# gtk2jwm
TclGTKfile="$HOME/.tcl-gtk"

# *taskbar*
tmpCL="$TMP_DIR/checklist.tmp.$$"
# variables
ISSET="true"
ISOFF="false"
SHRT="20"
NORM="28"
TALL="34"
XTTL="42"
TOP="0"
BTM="-1"
AUTO_OFF=ON
AUTO_SET=off
IS_SHRT=off
IS_NORM=ON
IS_TALL=off
IS_XTTL=off
CRHT="NORMAL"
AT_BTM=ON
AT_TOP=off

# trayInsert
DCL="<TaskListStyle insert=\"left\"/>"
DCR="<TaskListStyle insert=\"right\"/>"
DCO="<TaskListStyle insert="

# taskbarPlace
TOP="0"
BTM="-1"

# virtualDesk
MINTOPS=1
MAXTOPS=8
DC="<Desktops count=\"[0-9][0-9]*\"/>"
PF=

# focusModel
CLICK='<FocusModel>click</FocusModel>'
SLOPPY='<FocusModel>sloppy</FocusModel>'
tmp1P="$HOME/.jwm/jwmrc-personal-temp"
tmpIB="$TMP_DIR/inputbox.tmp.$$"
CHOICE=click
CLICK_ON=ON
SLOPPY_ON=off
VALUE="click"

# keyBoard
keyL="$TMP_DIR/keyL"
# chageKey
K=
M=
MASK="`echo $KEY | grep -c mask`"
A=
tmpSC="$TMP_DIR/shortcut"
ID=

# addShortcut
tmpAM="$TMP_DIR/addmask.tmp.$$"
tmpMK="$TMP_DIR/maskkey.tmp.$$"
MASKKEY=
InsSC='/bindings/a\'

# confirmKey
DEL="NO"
NEWKEY=$KY
ACTION=$A
OLDKEY=
OLDMASK=
OLD=
CHANGE_SED=
DEL_SED=
TEST=

#changekey
K=
M=
A=
tmpSCO="$TMP_DIR/shortcutold"
ID=
PATH1=$PATH
P="+"

#panel-buttons
POP="$TMP_DIR/tmp-popup.txt"
EXC="$TMP_DIR/tmp-exec.txt"
ICO="$TMP_DIR/tmp-icon.txt"
# Very End of this file 'usr/local/jwmconfig3/path' #
###END###

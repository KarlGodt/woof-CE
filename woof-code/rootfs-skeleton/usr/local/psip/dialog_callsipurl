#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_dialog_callsipurl"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/psip/dialog_callsipurl"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in 1; do shift; done; }

_trap

}
# End new header
#
# GPL v3 License
# PS VOIP for Puppy Linux v 0.7 June 27 008
# created by Lobster, CEL, HairyWill
# tested by Eric, Grant and the Puppy Community
###################

[ "$PJDEBUG" ] && echo started dialog_callsipurl

		export DIALOG='<window width="500"><vbox>
    	<text><label>Enter the sip address to call.</label></text>
  		<entry>
    	<variable>BUDDYNUM</variable>
    	<default>sip:</default>
  		</entry>
  		<hbox>
   		<button ok></button>
  		</hbox>
 		</vbox></window>'
		for STATEMENTS in $(gtkdialog3 --program DIALOG --name "PSIP Enter URL" ); do
  			eval $STATEMENTS
		done
	[ "$EXIT" = "OK" ] && echo -e "\nT\n3\nm\n$BUDDYNUM\n" >> $PJSIGNAL
	
[ "$PJDEBUG" ] && echo "finished dialog_call"

# Very End of this file 'usr/local/psip/dialog_callsipurl' #
###END###

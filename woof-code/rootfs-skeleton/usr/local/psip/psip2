#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_psip2"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/psip/psip2"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

TWO_HELP=''; TWO_VERSION=''; TWO_VERBOSE=''; TWO_DEBUG=''; ## Set to anything if code requires further down (ie. custom usage or version message)
ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in `seq 1 1 $DO_SHIFT`; do shift; done; }

_trap

}
# End new header
#

export PJCFG="$HOME/.psip/pjsua.cfg" # config-file
export PJAPPDIR=$(pwd)
export PJICONS=$PJAPPDIR/icons
export PJTMP=/tmp/psip
export PJSIGNAL=$PJTMP/pjsua-input #file for inputing commands
export PJLOG=$PJTMP/app-log
export PJDEBUG='anything' #to stop debugging, just leave this unset
export PJSUA=pjsua_custom_03-0.9.0


rm $PJTMP/buddy-list* 2>/dev/null
$PJAPPDIR/func_startpjsua &

#start the gui
echo -e "100\nStarting Psip" > $PJTMP/status-message
$PJAPPDIR/psip-gui
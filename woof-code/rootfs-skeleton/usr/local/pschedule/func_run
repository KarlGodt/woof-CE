#! /bin/bash
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_func_run"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/usr/local/pschedule/func_run"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in 1; do shift; done; }

_trap

}
# End new header
#
TREE=`cat /tmp/pschedule-treeitems | grep $TREE | cut -d "|" -f 3 | cut -c 2-` #jake_take 
COMMAND=`echo "$TREE" | cut -d " " -f 6-`
echo "$COMMAND" > /tmp/pschedule-run
chmod 777 /tmp/pschedule-run
/tmp/pschedule-run
# Very End of this file 'usr/local/pschedule/func_run' #
###END###

#!/bin/sh

. /etc/rc.d/f4puppy5

_debug "$0:$*"

while read line
do
[ "$line" ] || continue
case $line in \#*) continue;;esac
eval $line
which $AQUALUNG_EXE && break
done <<EoD
AQUALUNG_EXE=aqualung-new
AQUALUNG_EXE=aqualung2
AQUALUNG_EXE=/usr/multimedia/bin/aqualung
EoD

[ "$AQUALUNG_EXE" ] || exit 2
which $AQUALUNG_EXE || exit 2
pidof $AQUALUNG_EXE && exit 1

#killall $AQUALUNG_EXE 2>/dev/null

#if [ -f /usr/lib/libavcodec.so.51.44.0-orig ];then
##rm -f /usr/lib/libavcodec.so.51
#ln $VERB -sf libavcodec.so.51.44.0-orig /usr/lib/libavcodec.so.51
#fi

sleep 1
#exec aqualung2 "$@"
_debug "$0:$*"
exec $AQUALUNG_EXE "$@" &

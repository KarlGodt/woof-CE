#!/bin/sh
#
# New header by Karl Reimer Godt, September 2014
  _TITLE_="Puppy_ps"
_VERSION_=1.0omega
_COMMENT_="$_TITLE_:Puppy Linux shell script [to TODO here]"

MY_SELF="/bin/ps"
MY_PID=$$

test -f /etc/rc.d/f4puppy5 && {
[ "$HAVE_F4PUPPY5" ] || source /etc/rc.d/f4puppy5

ADD_PARAMETER_LIST=""
ADD_PARAMETERS=""
_provide_basic_parameters

TWO_HELP='1'; TWO_VERSION='1'; TWO_VERBOSE=''; TWO_DEBUG=''; ## Set to anything if code requires further down (ie. custom usage or version message)
ADD_HELP_MSG="$_COMMENT_"
_parse_basic_parameters "$@"
[ "$DO_SHIFT" ] && [ ! "${DO_SHIFT//[[:digit:]]/}" ] && {
  for oneSHIFT in `seq 1 1 $DO_SHIFT`; do shift; done; }

_trap

}
# End new header
#
#the full ps has different output to busybox ps if no params, for compatibility
#with our scripts, if no commandline params, run busybox ps...
if test "`tty`" = 'not a tty'; then
[ ! "$1" ] && exec busybox ps
[ "`which ps-FULL`" != "" ] && exec ps-FULL $@
elif [ "$*" ] ; then exec ps-FULL $@
else exec ps-FULL -A -H -w -o pid,ppid,lwp,tty,start,etime,s,psr,pcpu,args
fi

exec busybox ps
# Very End of this file 'bin/ps' #
###END###

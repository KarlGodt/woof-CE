#!/bin/ash
. /etc/rc.d/f4puppy5
# BATCHMARKER01 - Marker for Line-Position to bulk insert code into.

DEBUG=1
_debug "$*"
#Have set alias ps='/bin/ps -elF'
#in .bashrc . Though ash now, -elF gets passed
#as parameters
test "$DEBUG" && {
(
echo "alias:"
alias
alias ps
unalias ps
) >&2
}

case "$@" in -elF) :;;
*) OPTS=`echo $* | sed 's%-elF%%'`
   set - $OPTS
;;
esac

_debug "$*"

#the full ps has different output to busybox ps if no params, for compatibility
#with our scripts, if no commandline params, run busybox ps...
[ "$*" ] || exec busybox ps
[ "`which ps-FULL`" ] && exec ps-FULL "$@"
exec busybox ps "$@"

#!/bin/sh

aclocalcmd=aclocal

libtoolizecmd=libtoolize

autoheadercmd=autoheader

autoconfcmd=autoconf

automakecmd=automake  #--add-missing

$aclocalcmd --verbose 1>autogen_krg.log 2>&1

$libtoolizecmd -c --debug 1>>autogen_krg.log 2>&1

$autoheadercmd --verbose 1>>autogen_krg.log 2>&1

$autoconfcmd --verbose 1>>autogen_krg.log 2>&1

$automakecmd --add-missing -c --verbose 1>>autogen_krg.log 2>&1
